{
  "name": "ackee-node-logger",
  "version": "1.0.0",
  "description": "Ackee Node Logger",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --strict --noImplicitReturns --noUnusedLocals --noUnusedParameters",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "tslint -p . --force --format codeFrame; exit 0",
    "pretty": "prettier --write 'src/*.ts'",
    "precommit": "lint-staged && npm test",
    "check": "npm-check -i components -i config -i controllers -i enums -i errors -i middleware -i routes -i repositories -i services -i husky & exit 0"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "author": "Michal Vlas√°k <michal.vlasak@ackee.cz>",
  "license": "ISC",
  "engines": {
    "node": "6.14.3"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/lodash.isobject": "^3.0.4",
    "@types/lodash.isstring": "^4.0.4",
    "@types/on-finished": "^2.3.1",
    "@types/on-headers": "^1.0.0",
    "@types/pino": "^5.6.1",
    "@types/pino-multi-stream": "^3.1.1",
    "lodash.difference": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "omit-deep": "^0.3.0",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1",
    "pick-deep": "^1.0.0",
    "pino": "^4.13.0",
    "pino-multi-stream": "3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "eslint": "^4.18.2",
    "eslint-config-ackee": "^0.8.1",
    "express": "^4.16.3",
    "flush-write-stream": "^1.0.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-expect-message": "^1.0.1",
    "lint-staged": "^7.0.0",
    "npm-check": "^5.5.2",
    "prettier": "^1.14.3",
    "split2": "^2.2.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-ackee": "^0.2.1",
    "typescript": "^3.1.3"
  }
}
